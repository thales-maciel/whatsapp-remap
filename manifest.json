{
  "manifest_version": 2,
  "name": "WhatsApp Shortcuts",
  "version": "1.1",
  "description": "Custom shortcuts for WhatsApp Web",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/*"],
      "js": ["js/content.js"]
    }
  ],
  "options_ui": {
    "page": "options/options.html"
  }
}

